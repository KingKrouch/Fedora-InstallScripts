<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-x509-ca-cert">
        <comment>DER/PEM/Netscape-encoded X.509 certificate</comment>
        <glob-deleteall/>
        <glob pattern="*.cert"/>
        <glob pattern="*.crt"/>
        <glob pattern="*.der"/>
        <glob pattern="*.pem"/>
    </mime-type>
</mime-info>
